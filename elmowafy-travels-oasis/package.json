{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:cdn": "CDN_MODE=true npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "serve": "npm run build && npm run preview",
    "analyze": "npm run build:analyze && open dist/bundle-analysis.html",
    "size-limit": "size-limit",
    "perf:lighthouse": "lighthouse http://localhost:4173 --output=html --output-path=./reports/lighthouse.html",
    "perf:bundle": "webpack-bundle-analyzer dist/stats.json",
    "precommit": "npm run lint && npm run test",
    "clean": "rimraf dist && rimraf node_modules/.vite"
  },
  "dependencies": {
