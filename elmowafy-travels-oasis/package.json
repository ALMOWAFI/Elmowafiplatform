  "size-limit": [
    {
      "name": "Total Bundle Size",
      "path": "dist/assets/*.js",
      "limit": "3 MB"
    },
    {
      "name": "Initial Bundle",
      "path": "dist/assets/index-*.js",
      "limit": "500 KB"
    },
    {
      "name": "Vendor Libraries",
      "path": "dist/assets/*-libs-*.js",
      "limit": "1.5 MB"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
